

{
    
    "name": "chat-app",
    "version": "1.0.0",
    "description": "A minimal chat-app",
    "main": "main.js",
    "scripts": {
        "start": "electron .",
        "linux": "electron-builder --linux",
        "windows": "electron-builder --windows",
        "package": "electron-packager ./ --platform=all"
    },
    "repository": "https://github.com/electron/electron-quick-start",
    "keywords": [
        "Electron",
        "quick",
        "start",
        "tutorial",
        "demo"
    ],
    "author": "poojan <poojan.test@gmail.com>",
    "license": "MIT",
    "devDependencies": {
        "electron": "4.0.0",
        "electron-builder": "20.38.5"
    },
    "build": {
        "appId": "poojan-chat-appId",
        "linux": {
            "target": [
                "deb"
            ],
            "category": "Utility"
        },
        "win": {
            "target": "msi"
        }
    },
    "dependencies": {
        "electron-browser-storage": "^1.0.6",
        "electron-window-manager": "^1.0.6"
    }
}
